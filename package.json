{
    "name": "dx9lua",
    "description": "DX9WARE Lua support for Visual Studio Code.",
    "version": "0.2.1",
    "publisher": "RobloxArchiver",
    "icon": "assets/logo.png",
    "engines": {
        "vscode": "^1.89.1"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "contributes": {
        "languages": [
            {
                "id": "lua",
                "aliases": ["Lua", "dx9lua"],
                "extensions": [
                    ".dx9.lua",
                    ".dx9",
                    ".lua",
                    ".dx9lua"
                ]
            }
        ],
        "snippets": [
            {
                "language": "lua",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RobloxArchiver/dx9lua"
    }
}
